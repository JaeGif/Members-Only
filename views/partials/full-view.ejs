<div>
  <h1>Members Message Board</h1>
  <% if (baseView) { %>
  <a href="/home/new"><button type="button">New</button></a>

  <% } else { %> <%- include('./partials/new-post.ejs') %> <% } %> <% for( let i
  = 0; i < posts.length; i++ ) { %>
  <div>
    <h2><%= posts[i].title %></h2>
    <p><%= posts[i].message %></p>
  </div>
  <% } %>
</div>
